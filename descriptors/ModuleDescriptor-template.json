{
  "id": "@artifactId@-@version@",
  "name": "Job Manager",
  "provides": [
    {
      "id": "job-manager",
      "version": "1.0",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/api/v1/mergeJob",
          "permissionsRequired": [],
          "modulePermissions": []
        },
        {
          "methods": [
            "GET",
            "DELETE"
          ],
          "pathPattern": "/api/v1/mergeJob/{id}",
          "permissionsRequired": []
        },
        {
          "methods": [
            "GET",
            "POST"
          ],
          "pathPattern": "/api/v1/mergeJob/typeA",
          "permissionsRequired": []
        },
        {
          "methods": [
            "GET",
            "POST"
          ],
          "pathPattern": "/api/v1/mergeJob/typeB",
          "permissionsRequired": []
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "2.0",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/_/tenant"
        },
        {
          "methods": [
            "GET",
            "DELETE"
          ],
          "pathPattern": "/_/tenant/{id}"
        }
      ]
    }
  ],
  "launchDescriptor": {
    "dockerImage": "@artifactId@:@version@",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 357913941,
        "PortBindings": {
          "8081/tcp": [
            {
              "HostPort": "%p"
            }
          ]
        }
      }
    },
    "env": [
      {
        "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      },
      {
        "name": "DB_HOST",
        "value": "postgres"
      },
      {
        "name": "DB_PORT",
        "value": "5432"
      },
      {
        "name": "DB_USERNAME",
        "value": "folio_admin"
      },
      {
        "name": "DB_PASSWORD",
        "value": "folio_admin"
      },
      {
        "name": "DB_DATABASE",
        "value": "okapi_modules"
      },
      {
        "name": "DB_QUERYTIMEOUT",
        "value": "60000"
      },
      {
        "name": "DB_CHARSET",
        "value": "UTF-8"
      },
      {
        "name": "DB_MAXPOOLSIZE",
        "value": "5"
      }
    ]
  }
}
